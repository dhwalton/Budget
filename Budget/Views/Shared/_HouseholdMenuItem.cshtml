@model IEnumerable<Budgeter.Models.Household>
<li class="dropdown">
    <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">My Households <span class="caret"></span></a>
    <ul class="dropdown-menu primary">
        <li class="accent">@Html.ActionLink("Create New", "Create")</li>
        @if (Model.Count() > 0)
        {
            <li role="separator" class="divider"></li>
            foreach (var household in Model)
            {
                <li>@Html.ActionLink(household.Name, "Edit", "Households", new { id = household.Id }, null)</li>
            }
        }
    </ul>
</li>

    
    
